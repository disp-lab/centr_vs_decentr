				IloLinearNumExpr expr = cplex.linearNumExpr();
				expr.addTerm(1.0,	u[i]);
				expr.addTerm(-1.0,	u[j]);
				expr.addTerm(n,	x[i][j][k]);
				cplex.addLe(expr, n-1);
