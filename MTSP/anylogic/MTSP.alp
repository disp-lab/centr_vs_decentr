			for(int column_in_x = 0 ; column_in_x < n ; column_in_x++){
				if(solution_temp[position] != column_in_x)
					if(cplex.getValue( x[ solution_temp[position] ][ column_in_x ] ) > 0.9){
						solution_temp[position+1] = column_in_x;
						break;
					}
			}//for(column_in_x)
		}//for(position)

