					}
				}//for(l)
				expr.addTerm(-1.0,	x[i][j][k]);
				cplex.addGe(expr, 0);
			}//if(k)
		}//for(j)
	}//for(i)

	// constraint 8bis (lines 95-97 'maxChanges_of_allocations' in 5MTSP-individualisedReallocation)
