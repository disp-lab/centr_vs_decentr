		trace("\nu\t");
		for(int i=1 ; i<n ; i++)
			trace("\t"+cplex.getValue(u[i]));
*/
	}//if(cplex.solve)
//	traceln("\nEND of solve_cplex_mtsp");
	//Convert from 'x[][]' matrix into 'solution_temp[]'		
